{
    "bw-onkeypress": {
        "scope": "php,html",
        "prefix": "bw-onkeypress",
        "body": [
            "onkeypress=\"return /[a-zA-Z0-9@.\\ -]/i.test(event.key)\""
        ],
        "description": "onkeypress input check"
    },
    "bw-pattern": {
        "scope": "php,html",
        "prefix": "bw-pattern",
        "body": [
            "pattern=\"[A-Za-z]{3}\""
        ],
        "description": "input pattern. Read More here: https://www.w3schools.com/tags/att_input_pattern.asp"
    },
    "Print to console": {
        "prefix": "bw-any",
        "body": [
            "console.log('$1');",
            "$2"
        ],
        "description": "Log output to console"
    },
    "bw-mat-icon": {
        "prefix": "bw-mat-icon",
        "body": [
            "<mat-icon aria-hidden=\"false\" aria-label=\"${1:plane} icon\">${1:plane}</mat-icon>"
        ],
        "description": "Google Material Icon"
    },
    "bw-tabs": {
        "prefix": "bw-tabs",
        "body": [
            "<ul class=\"nav nav-tabs\" id=\"LoremIpsumTab\" role=\"tablist\">",
